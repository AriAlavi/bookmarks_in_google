{
    "name" : "Bookmarks in Google",
    "version": "1.0",
    "manifest_version": 2,
    "permissions" : [
        "bookmarks"
    ],
    "content_scripts" : [
        {
            "matches" : ["*://www.google.com/search?*"],
            "js" : ["google_bookmarker.js"]
        }
    ],
    "background" : {
        "scripts" : ["bookmark_retriever.js"],
        "persistent" : false
    }
}